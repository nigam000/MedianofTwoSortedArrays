class Solution(object):
    def findMedianSortedArrays(self, nums1, nums2):
        c=0
        num3=nums1+nums2
        num3.sort()
        if(len(num3)%2==0):
            c=(num3[(len(num3)/2)-1]+num3[(len(num3))/2])/2.0
        else:
            c=num3[(len(num3)/2)]
        print(num3)
        return c
